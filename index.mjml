                    <mj-navbar-link href="#" color="#112A5E">&vert;</mj-navbar-link>
                    <mj-navbar-link css-class="lato-font menu-link" padding="0" href="#" color="#112A5E">Rooms</mj-navbar-link>
                    <mj-navbar-link href="#" color="#112A5E">&vert;</mj-navbar-link>
                    <mj-navbar-link css-class="lato-font menu-link" padding="0" href="#" color="#112A5E">Blog</mj-navbar-link>
                    <mj-navbar-link href="#" color="#112A5E">&vert;</mj-navbar-link>
                    <mj-navbar-link css-class="lato-font menu-link" padding="0" href="#" color="#112A5E">Events</mj-navbar-link>
                </mj-navbar>
            </mj-column>
        </mj-section>

        <!-- Company Header -->
     

