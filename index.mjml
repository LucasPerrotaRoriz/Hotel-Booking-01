        </mj-attributes>
        <mj-style inline="inline">
            .lora-font div {
                font-family: 'Lora', sans-serif !important;
            }

            .lato-font div {
                font-family: 'Lato', sans-serif !important;
            }

            .menu-link {
                font-size: 14px;
                font-weight: bold;
                letter-spacing: 2px;
            }

            .align-to-middle div {
                text-align: center !important;
            }

            .h1-title div {
                font-size: 50px !important;
                font-weight: bold;
                color: #072b52 !important;
            }
        </mj-style>
        <mj-breakpoint width="600px" />
    </mj-head>
